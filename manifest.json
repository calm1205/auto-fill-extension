{
  "name": "Auto fill extension",
  "description": "お問い合わせ内容を自動で入力します。",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png"
    },
    "default_title": "Click Me",
    "default_popup": "popup.html"
  }
}
